<?xml version='1.0' encoding='utf-8'?>
<component
    name='mv_LayoutGroup'
    extends='LayoutGroup'>
  <interface>
    <!-- allows removing of subcomponents from layout measuring
    based on their visible flag -->
    <field
        id='style'
        type='assocarray' />
    <field
        id='__debugM'
        type='boolean'
        alwaysNotify='false' />

    <function name='_initialize' />
    <function name='getOriginalChildren' />
  </interface>
</component>